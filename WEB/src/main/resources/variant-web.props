# Session ID persister in cookie.
session.id.persister.class.name = com.variant.web.adapter.SessionIdPersisterHttpCookie

# Targeting persister in cookie.
targeting.persister.class.name = com.variant.web.adapter.TargetingPersisterHttpCookie

# Postgres Event Persister
event.persister.class.name = com.variant.core.ext.EventPersisterPostgres
event.persister.jdbc.vendor = postgres
event.persister.jdbc.url = jdbc:postgresql://localhost/variant
event.persister.jdbc.user = variant
event.persister.jdbc.password = variant
