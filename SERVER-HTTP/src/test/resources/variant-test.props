event.writer.buffer.size = 10000
event.writer.max.delay.millis = 100

session.store.class.init = {"apiEndpoint":"**Not used on Server**", "sessionTimeoutSecs":1, "vacuumingFrequencySecs":1}

# Event Persister (H2 in memory)
event.persister.class.name = com.variant.core.ext.EventPersisterH2
event.persister.class.init = {"url":"jdbc:h2:mem:variant;MVCC=true;DB_CLOSE_DELAY=-1;","user":"variant","password":"variant"}

